{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "generateToken.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    },
    {
      "src": "/generate_token",
      "dest": "generateToken.js"
    }
  ],
  "env": {
    "DEBUG": "true"
  }
}
